    "front-web-client": {
      "projectType": "application",
      "root": "apps/front/web-client",
      "sourceRoot": "apps/front/web-client/src",
      "prefix": "seed-web-client",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/front/web-client",
            "index": "apps/front/web-client/src/index.html",
            "main": "apps/front/web-client/src/main.ts",
            "polyfills": "apps/front/web-client/src/polyfills.ts",
            "tsConfig": "apps/front/web-client/tsconfig.app.json",
            "assets": [
              "apps/front/web-client/src/favicon.ico",
              "apps/front/web-client/src/assets",
              "apps/front/web-client/src/firebase-messaging-sw.js"
            ],
            "styles": ["apps/front/web-client/src/styles/index.sass"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/front/web-client/web/src/styles", "libs/front/shared/styles/src"]
            },
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/front/web-client/src/environments/environment.ts",
                  "with": "apps/front/web-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "front-web-client:build",
            "port": 4200
          },
          "configurations": {
            "production": {
              "browserTarget": "front-web-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front-web-client:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/front/web-client/src/**/*.ts", "apps/front/web-client/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/front/web-client/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/front/web-client"]
        },
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn build front-web-client --prod",
              "yarn firebase target:apply hosting front-web-client $FRONT_WEB_CLIENT_HOSTING_TARGET --project=$PROJECT_ID",
              "yarn firebase deploy --only hosting:front-web-client --project=$PROJECT_ID"
            ],
            "parallel": false,
            "color": true
          }
        }
      }
    },
