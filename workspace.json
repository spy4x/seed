          "outputs": ["coverage/apps/back/api"],
          "options": {
            "jestConfig": "apps/back/api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn build back-api --prod",
              "docker build . -t gcr.io/$PROJECT_ID/$CLOUD_RUN_API_APP_NAME -f back-api.dockerfile",
              "docker push gcr.io/$PROJECT_ID/$CLOUD_RUN_API_APP_NAME",
              "gcloud run deploy $CLOUD_RUN_API_APP_NAME --image gcr.io/$PROJECT_ID/$CLOUD_RUN_API_APP_NAME --add-cloudsql-instances $INSTANCE_CONNECTION_NAME --update-env-vars DB_CONNECTION_STRING=$DB_CONNECTION_STRING,API_URL=$API_URL,REGION=$REGION,PROJECT_ID=$PROJECT_ID --platform managed --region $REGION --allow-unauthenticated --project=$PROJECT_ID"
            ],
            "parallel": false,
            "color": true
          }
        }
      }
    },
