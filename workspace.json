    "front-web-client": {
      "projectType": "application",
      "root": "apps/front/web-client",
      "sourceRoot": "apps/front/web-client/src",
      "prefix": "seed-client",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/front/web-client",
            "index": "apps/front/web-client/src/index.html",
            "main": "apps/front/web-client/src/main.ts",
            "polyfills": "apps/front/web-client/src/polyfills.ts",
            "tsConfig": "apps/front/web-client/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/front/web-client/src/favicon.ico", "apps/front/web-client/src/assets"],
            "styles": ["apps/front/web-client/src/styles/index.sass"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/front/web-client/web/src/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/front/web-client/src/environments/environment.ts",
                  "with": "apps/front/web-client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "front-web-client:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "front-web-client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "front-web-client:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/front/web-client/tsconfig.app.json", "apps/front/web-client/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/front/web-client/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/front/web-client/jest.config.js",
            "passWithNoTests": true
