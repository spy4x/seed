{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "seed",
  "affected": {
    "defaultBase": "master"
  },
  "cli": {
    "packageManager": "yarn"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "YWVlYTk3ODgtZWY3OC00MGFhLWEyNTUtOWE0NTI3ODBmMWE3fHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    }
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "sass",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "sass",
      "inlineStyle": true,
      "spec": true,
      "flat": true,
      "changeDetection": "OnPush"
    }
  },
  "defaultProject": "front-web"
}
